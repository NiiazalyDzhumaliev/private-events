<p><strong><%=@user.username%></strong></p>

<table class="table">
  <tr class="thead-dark">  
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Date</th>
  </tr>
  <% @user.attended_events.each do |event| %>
   <tr>
    <td><%= event.title %></td>
    <td><%= event.desc %></td>
    <td><%= event.location %></td>
    <td><%= event.date %></td>
  </tr>
<%end%>
</table>
